{
  "name": "gulp_compiler",
  "version": "1.0.0",
  "description": "To build the compiler to parse .pug, .sass, .ES6+, compressing photographics.",
  "main": "index.js",
  "scripts": {
    "run": "gulp",
    "build": "gulp build --env build",
    "css": "gulp sass --env build",
    "vendors": "gulp vendors --env build",
    "js": "gulp babel --env build",
    "html": "gulp pug --env build",
    "assets": "gulp assets --env build",
    "move": "gulp move --env build",
    "inlineSVG": "gulp inlineSVG",
    "sprite": "gulp sprite",
    "commit": "git add dist && git commit -m \"Initial dist subtree commit\""
  },
  "pre-commit": [
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/register": "^7.10.5",
    "autoprefixer": "^9.8.6",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.12",
    "del": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-inline-svg": "^1.2.0",
    "gulp-load-plugins": "^1.6.0",
    "gulp-order": "^1.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-purgecss": "^1.2.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^4.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "main-bower-files": "^2.13.3",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "gh-pages": "^6.0.0"
  },
  "browserslist": [
    "last 1 version"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
